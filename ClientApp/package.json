{
  "name": "JobMatcher",
  "version": "1.0.0",
  "description": "Job Matcher website",
  "author": "Van Vu",
  "private": true,
  "scripts": {
    "dev": "npm run build:ts && node server",
    "build": "npm run build:ts && npm run build:server && npm run build:client && cross-env NODE_ENV='production' node server",
    "build:ts": "tsc -p .",
    "build:client": "cross-env NODE_ENV='production' webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV='production' webpack --config build/webpack.server.config.js --progress --hide-modules",
    "jest": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "class-transformer": "^0.1.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "lru-cache": "^4.1.1",
    "object-path": "^0.11.4",
    "reflect-metadata": "^0.1.10",
    "vue": "^2.5.3",
    "vue-class-component": "^6.1.0",
    "vue-meta": "^1.5.0",
    "vue-property-decorator": "^6.0.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.3",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "@types/node": "^8.0.0",
    "@vue/test-utils": "^1.0.0-beta.11",
    "babel-core": "^6.25.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "compression": "^1.7.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.26.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "file-loader": "^0.10.0",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "function-bind": "^1.1.0",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^2.0.1",
    "jest": "^22.1.4",
    "jest-vue-preprocessor": "^1.3.1",
    "lodash.merge": "^4.6.0",
    "node-sass": "^4.6.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.1.0",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.3",
    "sinon": "^4.2.2",
    "style-loader": "^0.16.1",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-jest": "^22.0.1",
    "ts-loader": "^2.0.1",
    "typescript": "2.5.1",
    "uglify-js": "^2.8.29",
    "url-loader": "^0.5.7",
    "vue-loader": "^13.5.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "vue-typescript-import-dts": "4.0.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "vue",
      "ts"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
